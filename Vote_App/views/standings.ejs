<%include header%>

<body>
    <%include navbar%>
    <div class='container'>
        <div class='row'>
            <div class="col-sm-6 col-sm-offset-3">
                <h1>How domestic is each animal?</h1>
                <% results.forEach((animal)=>{%>
                    <div class='species-name'>
                        <%=animal.species%>
                        <%=animal.domesticCount%>
                    </div>
                <%})%>
            </div>
        </div>
    </div>    
</body>
</html>